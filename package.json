{
  "name": "vue-ts-quick",
  "version": "0.1.0",
  "author": {
    "name": "b0yblake",
    "email": "lichnh.fe@gmail.com",
    "url": "http://lich.work/"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "eslint ./src --ext .vue,.js,.ts",
    "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts",
    "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "test": "jest ./__test__",
    "test:unit": "jest test:unit --coverage",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0 -s",
    "commit": "npx cz",
    "docs": "typedoc --out ./docs/ ./src/ && npm run serve:docs",
    "serve:docs": "http-server ./docs -p 3001 -o",
    "analysis": "cross-env use_analyzer=true vue-cli-service build",
    "prepare": "husky install"
  },
  "main": "dist/index.js",
  "files": [
    "@types"
  ],
  "license": "MIT",
  "types": "./@types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/b0yblake/vue-ts-quick"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "@vitejs/plugin-vue-jsx": "^1.1.5",
    "@vue/test-utils": "^2.0.0-rc.4",
    "@vueuse/core": "^5.1.3",
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "es6-promise": "^4.2.8",
    "include-media": "^1.4.10",
    "lodash-es": "^4.17.21",
    "vite-plugin-components": "^0.11.1",
    "vue": "^3.0.5",
    "vue-axios": "^3.2.4",
    "vue-i18n": "^8.24.4",
    "vue-router": "^4.0.9",
    "vuex": "^4.0.2",
    "vuex-persistedstate": "^4.0.0-beta.3"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/line-clamp": "^0.2.0",
    "@tailwindcss/typography": "^0.4.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/vue-fontawesome": "^3.0.0-1",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "autoprefixer": "^10.2.6",
    "commitizen": "^4.2.3",
    "eslint": "^7.28.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-vue": "^7.11.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "node-sass": "^6.0.0",
    "postcss": "^8.3.5",
    "prettier": "^2.3.1",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "tailwindcss": "^2.2.0",
    "typescript": "^4.3.3",
    "vite": "^2.3.7",
    "vue-jest": "^5.0.0-alpha.7",
    "ts-jest": "^26.5.4",
    "vue-tsc": "^0.0.24"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  }
}
